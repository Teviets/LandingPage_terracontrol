generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ContactRequest {
  id        Int      @id @default(autoincrement())
  email     String
  message   String
  source    String?
  createdAt DateTime @default(now())
}

model User {
  id            Int      @id @default(autoincrement())
  username      String   @unique
  password      String
  hasFullAccess Int
  @@map("users")
}
